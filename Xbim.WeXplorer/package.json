{
  "name": "@habx/xbim-viewer",
  "version": "2.2.1",
  "main": "Build/xbim-viewer.js",
  "devDependencies": {
    "dts-bundle": "^0.7.2",
    "jsdoc": "3.3.0",
    "ts-loader": "2.0.3",
    "typescript": "2.3.2",
    "webpack": "^2.4.1",
    "webpack-cli": "^3.1.2",
    "worker-loader": "^0.8.0"
  },
  "scripts": {
    "build": "npm run jsdoc && npm run build:dev && npm run build:prod",
    "jsdoc": "jsdoc -c jsdoc.config.json",
    "build:dev": "cross-env NODE_ENV=development webpack -d",
    "build:prod": "cross-env NODE_ENV=production webpack -p"
  },
  "files": [
    "Browser/*",
    "Build/*",
    "Plugins/*",
    "Viewer/*",
    "Libs/*",
    "*.js*",
    "*.ts",
    "*.d.ts"
  ]
}
